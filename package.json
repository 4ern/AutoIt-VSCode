{
	"name": "autoit",
	"displayName": "AutoIt",
	"description": "AutoIt Script",
	"version": "0.0.5",
	"publisher": "LoganCH",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"devDependencies": {
		"vscode": "^0.11.x"
	},
	"main": "./extension",
	"activationEvents": [
			"onCommand:extension.runScript",
			"onCommand:extension.launchHelp",
			"onCommand:extension.launchInfo"
	],
	"contributes": {
		"languages": [{
			"id": "autoit",
			"aliases": ["AutoIt Script", "autoit"],
			"extensions": [".au3"],
			"configuration": "./autoit-language-configuration.json"
		}],
		"commands": [{ 
			"command": "extension.runScript",
			"title": "AutoIt - Run Script" 
		}, {
			"command": "extension.launchHelp",
			"title": "AutoIt - Help"
		}, {
			"command": "extension.launchInfo",
			"title": "AutoIt - Au3Info"
		}],
		"grammars": [{
			"language": "autoit",
			"scopeName": "source.autoit",
			"path": "./syntaxes/autoit.tmLanguage"
		}],
		"snippets": [
			{
				"language": "autoit",
				"path": "./syntaxes/snippets.json"
			}
		],
		"keybindings": [{
			"command": "extension.runScript",
			"key": "f5",
			"when": "editorTextFocus"
		}, {
			"command": "extension.launchHelp",
			"key": "ctrl+f1",
			"when": "editorTextFocus"
		}, {
			"command": "extension.launchInfo",
			"key": "ctrl+f6",
			"when": "editorTextFocus"
		}]
	}
}